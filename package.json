{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --https",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "tsc && vite build && gh-pages -d dist",
    "lint": "eslint --ext .ts,.tsx .",
    "jest": "jest",
    "cypress:open": "cypress open",
    "delete:reports": "rm cypress/results/* mochawesome-report/* || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "test": "npm run jest && npm run report && npx marge --showFailed cypress"
  },
  "dependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.11",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "classnames": "^2.3.2",
    "hls.js": "^1.3.4",
    "jest-environment-jsdom": "^29.5.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "react-test-renderer": "^18.2.0",
    "react-toggle-dark-mode": "^1.1.1",
    "redux-thunk": "^2.4.2",
    "sass": "^1.58.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^14.0.0",
    "@types/hls.js": "^1.0.0",
    "@types/lodash": "^4.14.191",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cypress": "^12.11.0",
    "cypress-mochawesome-reporter": "^3.4.0",
    "cypress-multi-reporters": "^1.6.3",
    "eslint": "^8.34.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
